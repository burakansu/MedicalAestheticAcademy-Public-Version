                                        <tbody>

                                            @foreach (var obj in order.Take(20))
                                            {
                                                @switch (obj.Product.Type)
                                                {
                                                    case 1:
                                                        les = baselesson.Single(x => x.LessonId == obj.Product.ProductId);
                                                        break;
                                                    case 2:
                                                        pck = basepackage.Single(x => x.PackageId == obj.Product.ProductId);
                                                        break;
                                                    case 3:
                                                        adv = baseadvisor.Single(x => x.AdvisorId == obj.Product.ProductId);
                                                        break;
                                                }
                                                @if (obj.Product.Type == 1)
                                                {
                                                    <tr>
                                                        <td>
                                                            <div class="d-flex align-items-center">
                                                                <div class="symbol symbol-50px me-3">
