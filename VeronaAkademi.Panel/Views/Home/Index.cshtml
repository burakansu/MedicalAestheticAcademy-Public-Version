@{
    ViewData["Title"] = "Anasayfa";


    string typestring = "";
    var baselecturer = MetaData.baseLecturer();
    var baseprofession = MetaData.baseProfession();
    var baselesson = MetaData.baseLesson();
    var baseskill = MetaData.baseSkill();
    var basetrailer = MetaData.baseTrailer();
    var order = MetaData.baseOrder();
    var basepackage = MetaData.basePackage();
    var baseadvisor = MetaData.baseAdvisor();
    var basecustomer = MetaData.baseCustomer();

    Lesson les = new Lesson();
    Package pck = new Package();
    Advisor adv = new Advisor();

    int sum = 0;

    foreach (var r in order)
    {
        switch (r.Product.Type)
        {
            case 1:
                sum += baselesson.Single(x => x.LessonId == r.Product.ProductId).Price;
                break;
            case 2:
                sum += basepackage.Single(x => x.PackageId == r.Product.ProductId).Price;
                break;
            case 3:
                sum += baseadvisor.Single(x => x.AdvisorId == r.Product.ProductId).Price;
                break;
        }
    }
}
